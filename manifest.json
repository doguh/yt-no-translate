{
  "description": "Prevents YouTube from translating video titles",
  "manifest_version": 2,
  "name": "YouTube No Translate",
  "version": "4.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["index.js"]
    }
  ]
}
